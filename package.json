{
  "name": "notion2anki",
  "description": "Go from Notion togglelists to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki",
    "imba"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.2.21",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "main.js",
  "scripts": {
    "dev": "snowpack dev",
    "test": "imba test/create_deck.imba",
    "build": "snowpack build",
    "start": "yarn dev",
    "build-server": "yarn run imbac src/server/server.imba -o .",
    "watch-server": "yarn run imbac -w src/server/server.imba -o .",
    "dev-server": "yarn run imba src/server/server.imba"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "bulma-steps": "^2.2.1",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "imba": "^2.0.0-alpha.95",
    "jszip": "^3.5.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12"
  },
  "build": {
    "appId": "com.alemayhu.notion2anki",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "main.js",
      "dist/app.js",
      "dist/index.html",
      "build",
      "node_modules"
    ]
  },
  "devDependencies": {
    "child_process": "^1.0.2",
    "esbuild": "^0.8.11",
    "imba-snowpack": "^0.0.8",
    "snowpack": "^2.17.1"
  }
}
